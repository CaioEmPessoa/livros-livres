{
  "configurations": [
    {
      "type": "java",
      "name": "ServerApplication",
      "request": "launch",
      "mainClass": "com.livros_livres.Server.ServerApplication",
      "projectName": "Server",
      "env": {
        // Enviromnent options. The info in () are the defaults and can be left blank (but it may not work)
        // (null) = No default info. You have to provide it yourself.
        "ACTIVE_PROFILE": "dev", // "dev" or "prod". Changes the project port and debug information displayed (dev)
        "DATABASE_PORT": "3306", // MySql server port (3306)
        "DATABASE_NAME": "livrosLivres", // Database name in mysql (livrosLivres)
        "DATABASE_USER": "root", // User that access mysql database (root)
        "DATABASE_PASSWORD": "",  // Database password (null)
        "MAIL_SENDER_ADDRESS": "", // endereço de email do livroslivres (null)
        "MAIL_SENDER_PASSWORD": "" // senha do app do gmail do livroslivres (sem espaços) (null)
      }
    }
  ]
}